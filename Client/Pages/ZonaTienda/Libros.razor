@page "/Tienda/Libros/{IdMateria:int}/{NombreMateria?}"@inject IRestService _rest<div class="row row-cols-1 row-cols-md-3">    @for (int i = 0; i < this.libros.Count; i = i + 2)    {        <Tupla3Libros ValorTupla3Libros="@(this.libros.Skip<Libro>(i).Take<Libro>(3).ToList<Libro>())"></Tupla3Libros>    }</div>@code {    [Parameter]    public int IdMateria { get; set; }    [Parameter]    public String NombreMateria { get; set; }    private List<Libro> libros = new List<Libro>();    protected override async Task OnInitializedAsync()    {        this.libros = await this._rest.DevolverLibros(this.IdMateria);    }}